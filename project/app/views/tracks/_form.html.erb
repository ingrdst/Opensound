<%= form_for ([@sound, @track]) do |f| %>
        <%= f.label :title %>
        <br>
        <%= f.text_field :title %>

        <br><br>

        <%= f.label :description %>
        <%= f.text_area :description %>

        <br><br>

        <div class="actions">
        <%= f.submit "Submit", class: "button button_highlight button_block" %>
        </div>

<% end %>